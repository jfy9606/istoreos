
#include "rk3576-vpu.dtsi"
#include "rk3576-npu.dtsi"
#include "rk3576-gpu.dtsi"

&gpu {
	reg = <0x0 0x27800000 0x0 0x20000>;
};

&otp {
	test_version: test-version@7 {
		reg = <0x7 0x1>;
		bits = <0 4>;
	};
	specification_serial_number: specification-serial-number@8 {
		reg = <0x08 0x1>;
		bits = <0 5>;
	};
};

#if 0
// NPU power domain cannot be registered (so no one will switch it off), that will crash system when rknpu probing
// [   12.673716] rockchip-pm-domain 27380000.power-management:power-controller: failed to get ack on domain 'nputop', val=0x11bff
&power {
	/delete-node/ power-domain@RK3576_PD_NPU;
};

&rknpu {
	/delete-property/ power-domains;
	/delete-property/ power-domain-names;
};
#endif

