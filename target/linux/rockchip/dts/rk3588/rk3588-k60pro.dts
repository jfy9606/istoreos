
/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/pinctrl/rockchip.h>
#include <dt-bindings/usb/pd.h>
#include <rockchip/rk3588.dtsi>
#include "rk3588-rk806-single.dtsi"
#include "rk3588s-ip.dtsi"
#include "rk3588s-ip-supply.dtsi"

/ {
	model = "ROCEOS K60pro";
	compatible = "roceos,k60pro", "rockchip,rk3588";

	aliases {
		ethernet0 = &gmac0;
		ethernet1 = &gmac1;

		led-boot = &led_red;
		led-failsafe = &led_red;
		led-running = &led_red;
		led-upgrade = &led_red;
	};

	chosen {
		stdout-path = "serial2:115200n8";
	};

	leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&led_net_en>, <&led_sata_en>,
			<&led_work_en>;

		net {
			label = "blue:net";
			gpios = <&gpio1 RK_PA3 GPIO_ACTIVE_HIGH>;
		};

		disk {
			label = "yellow:disk";
			gpios = <&gpio1 RK_PB2 GPIO_ACTIVE_HIGH>;
		};

		led_red: work {
			label = "red:work";
			gpios = <&gpio1 RK_PA4 GPIO_ACTIVE_HIGH>;
		};
	};

	vcc12v_dcin: vcc12v-dcin {
		compatible = "regulator-fixed";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <12000000>;
		regulator-max-microvolt = <12000000>;
		regulator-name = "vcc12v_dcin";
	};

	vcc5v0_sys: vcc5v0-sys {
		compatible = "regulator-fixed";
		regulator-always-on;
		regulator-boot-on;
		enable-active-high;
		gpios = <&gpio0 RK_PC0 GPIO_ACTIVE_HIGH>;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		regulator-name = "vcc5v0_sys";
		pinctrl-names = "default";
		pinctrl-0 = <&vcc5v0_power_en>;
		vin-supply = <&vcc12v_dcin>;
	};

	vcc3v3_sys: vcc3v3-sys {
		compatible = "regulator-fixed";
		regulator-always-on;
		regulator-boot-on;
		enable-active-high;
		gpios = <&gpio0 RK_PB7 GPIO_ACTIVE_HIGH>;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-name = "vcc3v3_sys";
		pinctrl-names = "default";
		pinctrl-0 = <&vcc3v3_power_en>;
		vin-supply = <&vcc12v_dcin>;
	};

	vcc_1v1_nldo_s3: vcc-1v1-nldo-s3 {
		compatible = "regulator-fixed";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <1100000>;
		regulator-max-microvolt = <1100000>;
		regulator-name = "vcc_1v1_nldo_s3";
		vin-supply = <&vcc5v0_sys>;
	};

	/* pi6c pcie clock generator feeds both ports */
	vcc3v3_pi6c_05: vcc3v3-pi6c-05 {
		compatible = "regulator-fixed";
		regulator-name = "vcc3v3_pi6c_05";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		enable-active-high;
		gpios = <&gpio4 RK_PA4 GPIO_ACTIVE_HIGH>;
		startup-delay-us = <200000>;
		pinctrl-names = "default";
		pinctrl-0 = <&pi6c_power_en>;
		vin-supply = <&vcc5v0_sys>;
	};

	/* actually fed by vcc5v0_sys, dependent on pi6c clock generator */
	vcc3v3_m2: vcc3v3-m2 {
		compatible = "regulator-fixed";
		enable-active-high;
		gpios = <&gpio4 RK_PA3 GPIO_ACTIVE_HIGH>;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-name = "vcc3v3_m2";
		startup-delay-us = <50000>;
		pinctrl-names = "default";
		pinctrl-0 = <&m2_power_en>;
		clk-supply = <&vcc3v3_pi6c_05>;
		vin-supply = <&vcc5v0_sys>;
	};

	/* actually fed by vcc3v3_sys, dependent on pi6c clock generator */
	vcc3v3_pcie: vcc3v3-pcie {
		compatible = "regulator-fixed";
		regulator-name = "vcc3v3_pcie";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		startup-delay-us = <50000>;
		clk-supply = <&vcc3v3_pi6c_05>;
		vin-supply = <&vcc3v3_sys>;
	};

	vcc_sata_pwr: vcc-sata-pwr {
		compatible = "regulator-fixed";
		enable-active-high;
		gpios = <&gpio1 RK_PA2 GPIO_ACTIVE_HIGH>;
		regulator-name = "vcc_sata_pwr";
		pinctrl-names = "default";
		pinctrl-0 = <&vcc_sata_pwr_en>;
		vin1-supply = <&vcc12v_dcin>;
		vin2-supply = <&vcc5v0_sys>;
	};

	vcc3v8_modem: vcc3v3-modem {
		compatible = "regulator-fixed";
		enable-active-high;
		regulator-always-on;
		regulator-boot-on;
		gpios = <&gpio3 RK_PD5 GPIO_ACTIVE_HIGH>;
		regulator-min-microvolt = <3800000>;
		regulator-max-microvolt = <3800000>;
		regulator-name = "vcc3v8_modem";
		pinctrl-names = "default";
		pinctrl-0 = <&modem_power_en>;
		vin-supply = <&vcc5v0_sys>;
	};

	/* press modem power key 2s to boot */
	modem-pwr-on {
		compatible = "regulator-fixed";
		enable-active-high;
		regulator-always-on;
		regulator-boot-on;
		gpios = <&gpio1 RK_PA0 GPIO_ACTIVE_HIGH>;
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-name = "modem-pwr-on";
		vccio-supply = <&vcc_1v8_s0>;
		startup-delay-us = <2000000>;
		pinctrl-names = "default";
		pinctrl-0 = <&modem_pwr_on_en>;
	};

	vcc5v0_otg: vcc5v0-usb-host {
		compatible = "regulator-fixed";
		enable-active-high;
		gpio = <&gpio3 RK_PA6 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&vcc5v0_otg_en>;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		regulator-name = "vcc5v0_otg";
		vin-supply = <&vcc5v0_sys>;
	};

	wireless_bluetooth: wireless-bluetooth {
		status = "okay";
		compatible = "bluetooth-platdata";
		clocks = <&hym8563>;
		clock-names = "ext_clock";
		uart_rts_gpios = <&gpio1 RK_PC2 GPIO_ACTIVE_LOW>;
		pinctrl-names = "default", "rts_gpio";
		pinctrl-0 = <&uart3_rtsn>, <&bt_reset_gpio>, <&bt_wake_gpio>, <&bt_irq_gpio>;
		pinctrl-1 = <&uart3_gpios>;
		BT,reset_gpio = <&gpio1 RK_PA7 GPIO_ACTIVE_HIGH>;
		BT,wake_gpio = <&gpio0 RK_PA0 GPIO_ACTIVE_HIGH>;
		BT,wake_host_irq = <&gpio1 RK_PA6 GPIO_ACTIVE_HIGH>;
	};

	wireless_wlan: wireless-wlan {
		status = "okay";
		compatible = "wlan-platdata";
		wifi_chip_type = "ap6255";
		pinctrl-names = "default";
		pinctrl-0 = <&wifi_host_wake_irq>;
		WIFI,host_wake_irq = <&gpio0 RK_PB0 GPIO_ACTIVE_HIGH>;
	};

	sdio_pwrseq: sdio-pwrseq {
		status = "okay";
		compatible = "mmc-pwrseq-simple";
		clocks = <&hym8563>;
		clock-names = "ext_clock";
		pinctrl-names = "default";
		pinctrl-0 = <&wifi_enable_h>;
		post-power-on-delay-ms = <200>;
		reset-gpios = <&gpio0 RK_PB2 GPIO_ACTIVE_LOW>;
	};

};

&combphy0_ps {
	status = "okay";
};

&combphy1_ps {
	status = "okay";
};

&combphy2_psu {
	status = "okay";
};

&cpu_l0 {
	cpu-supply = <&vdd_cpu_lit_s0>;
};

&cpu_l1 {
	cpu-supply = <&vdd_cpu_lit_s0>;
};

&cpu_l2 {
	cpu-supply = <&vdd_cpu_lit_s0>;
};

&cpu_l3 {
	cpu-supply = <&vdd_cpu_lit_s0>;
};

&cpu_b0 {
	cpu-supply = <&vdd_cpu_big0_s0>;
};

&cpu_b1 {
	cpu-supply = <&vdd_cpu_big0_s0>;
};

&cpu_b2 {
	cpu-supply = <&vdd_cpu_big1_s0>;
};

&cpu_b3 {
	cpu-supply = <&vdd_cpu_big1_s0>;
};

&gmac0 {// ethernet@fe1b0000
	/* Use rgmii-rxid mode to disable rx delay inside Soc */
	phy-mode = "rgmii-rxid";

	snps,reset-gpio = <&gpio3 RK_PC6 GPIO_ACTIVE_LOW>;
	snps,reset-active-low;
	/* Reset time is 20ms, 100ms for rtl8211f */
	snps,reset-delays-us = <0 20000 100000>;

	pinctrl-names = "default";
#if 0
	pinctrl-0 = <&gmac0_miim
			&gmac0_tx_bus2
			&gmac0_rx_bus2
			&gmac0_rgmii_clk
			&gmac0_rgmii_bus
			&gmac0_clkinout>;
	clock_in_out = "input";
#else
	pinctrl-0 = <&gmac0_miim
			&gmac0_tx_bus2
			&gmac0_rx_bus2
			&gmac0_rgmii_clk
			&gmac0_rgmii_bus>;
	clock_in_out = "output";
#endif
	tx_delay = <0x44>;
	// rx_delay = <0x00>;

	phy-handle = <&rgmii_phy0>;
	status = "okay";
};

&gmac1 {// ethernet@fe1c0000
	/* Use rgmii-rxid mode to disable rx delay inside Soc */
	phy-mode = "rgmii-rxid";

	snps,reset-gpio = <&gpio3 RK_PB2 GPIO_ACTIVE_LOW>;
	snps,reset-active-low;
	/* Reset time is 20ms, 100ms for rtl8211f */
	snps,reset-delays-us = <0 20000 100000>;

	pinctrl-names = "default";
#if 0
	pinctrl-0 = <&gmac1_miim
			&gmac1_tx_bus2
			&gmac1_rx_bus2
			&gmac1_rgmii_clk
			&gmac1_rgmii_bus
			&gmac1_clkinout>;
	clock_in_out = "input";
#else
	pinctrl-0 = <&gmac1_miim
			&gmac1_tx_bus2
			&gmac1_rx_bus2
			&gmac1_rgmii_clk
			&gmac1_rgmii_bus>;
	clock_in_out = "output";
#endif

	tx_delay = <0x30>;
	/* rx_delay = <0x4f>; */

	phy-handle = <&rgmii_phy1>;
	status = "okay";
};

&i2c0 {
	pinctrl-names = "default";
	pinctrl-0 = <&i2c0m2_xfer>;
	status = "okay";

	vdd_cpu_big0_s0: regulator@42 {
		compatible = "rockchip,rk8602";
		reg = <0x42>;
		fcs,suspend-voltage-selector = <1>;
		regulator-name = "vdd_cpu_big0_s0";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <550000>;
		regulator-max-microvolt = <1050000>;
		regulator-ramp-delay = <2300>;
		vin-supply = <&vcc5v0_sys>;

		regulator-state-mem {
			regulator-off-in-suspend;
		};
	};

	vdd_cpu_big1_s0: regulator@43 {
		compatible = "rockchip,rk8603", "rockchip,rk8602";
		reg = <0x43>;
		fcs,suspend-voltage-selector = <1>;
		regulator-name = "vdd_cpu_big1_s0";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <550000>;
		regulator-max-microvolt = <1050000>;
		regulator-ramp-delay = <2300>;
		vin-supply = <&vcc5v0_sys>;

		regulator-state-mem {
			regulator-off-in-suspend;
		};
	};
};

&i2c1 {
	pinctrl-names = "default";
	pinctrl-0 = <&i2c1m2_xfer>;
	status = "okay";

	vdd_npu_s0: regulator@42 {
		compatible = "rockchip,rk8602";
		reg = <0x42>;
		fcs,suspend-voltage-selector = <1>;
		regulator-name = "vdd_npu_s0";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <550000>;
		regulator-max-microvolt = <950000>;
		regulator-ramp-delay = <2300>;
		vin-supply = <&vcc5v0_sys>;

		regulator-state-mem {
			regulator-off-in-suspend;
		};
	};

};

&i2c5 {
	pinctrl-names = "default";
	pinctrl-0 = <&i2c5m2_xfer>;
	status = "okay";
};

&i2c6 {
	pinctrl-names = "default";
	pinctrl-0 = <&i2c6m0_xfer>;
	status = "okay";

	hym8563: rtc@51 {
		compatible = "haoyu,hym8563";
		reg = <0x51>;
		#clock-cells = <0>;
		clock-output-names = "hym8563";
		wakeup-source;
	};
};

&i2c7 {
	pinctrl-names = "default";
	pinctrl-0 = <&i2c7m3_xfer>;
	status = "okay";
};

&mdio0 {
	rgmii_phy0: ethernet-phy@0 {
		/* RTL8211FS */
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <0x0>;
		pinctrl-names = "default";
		pinctrl-0 = <&rtl8211f0_rst>;
		reset-assert-us = <20000>;
		reset-deassert-us = <100000>;
		//reset-gpios = <&gpio3 RK_PC6 GPIO_ACTIVE_LOW>;
		realtek,fiber;
		realtek,led-data = <0x616b>;
		realtek,flcr-data = <0xd3e9>;
	};
};

&mdio1 {
	rgmii_phy1: ethernet-phy@0 {
		/* RTL8211FS */
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <0x0>;
		pinctrl-names = "default";
		pinctrl-0 = <&rtl8211f1_rst>;
		reset-assert-us = <20000>;
		reset-deassert-us = <100000>;
		//reset-gpios = <&gpio3 RK_PB2 GPIO_ACTIVE_LOW>;
		realtek,fiber;
		realtek,led-data = <0x616b>;
		realtek,flcr-data = <0xd3e9>;
	};
};

&pcie2x1l0 {
	// idx3,p0l1
	phys = <&pcie30phy>;
	reset-gpios = <&gpio3 RK_PD4 GPIO_ACTIVE_HIGH>;
	vpcie3v3-supply = <&vcc3v3_pcie>;
	status = "okay";
};

&pcie2x1l1 {
	// idx4, p1l1
	phys = <&pcie30phy>;
	reset-gpios = <&gpio4 RK_PB6 GPIO_ACTIVE_HIGH>;
	vpcie3v3-supply = <&vcc3v3_pcie>;
	status = "okay";
};

&pcie2x1l2 {
	// m2
	reset-gpios = <&gpio3 RK_PC1 GPIO_ACTIVE_HIGH>;
	vpcie3v3-supply = <&vcc3v3_m2>;
	status = "okay";
};

&pcie30phy {
	// rockchip,pcie30-phymode = <PHY_MODE_PCIE_NABIBI>;
	data-lanes = <1 3 2 4>;
	status = "okay";
};

&pcie3x2 {
	// idx2, p1l0
	num-lanes = <1>;
	reset-gpios = <&gpio4 RK_PA2 GPIO_ACTIVE_HIGH>;
	vpcie3v3-supply = <&vcc3v3_pcie>;
	status = "okay";
};

&pcie3x4 {
	// idx1, p0l0
	num-lanes = <1>;
	reset-gpios = <&gpio3 RK_PD1 GPIO_ACTIVE_HIGH>;
	vpcie3v3-supply = <&vcc3v3_pcie>;
	status = "okay";
};

&pinctrl {
	rtl8211f {
		rtl8211f0_rst: rtl8211f0-rst {
			rockchip,pins = <3 RK_PC6 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		rtl8211f1_rst: rtl8211f1-rst {
			rockchip,pins = <3 RK_PB2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
	leds {
		led_net_en: led_net_en {
			rockchip,pins = <1 RK_PA3 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		led_sata_en: led_sata_en {
			rockchip,pins = <1 RK_PB2 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		led_work_en: led_work_en {
			rockchip,pins = <1 RK_PA4 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	modem {
		modem_power_en: modem-power-en {
			rockchip,pins = <3 RK_PD5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		modem_pwr_on_en: modem-pwr-on-en {
			rockchip,pins = <1 RK_PA0 RK_FUNC_GPIO &pcfg_pull_down>;
		};
	};
	pi6c {
		pi6c_power_en: pi6c-power-en {
			rockchip,pins = <4 RK_PA4 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
	m2 {
		m2_power_en: m2-power-en {
			rockchip,pins = <4 RK_PA3 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
	power {
		vcc3v3_power_en: vcc3v3_power_en {
			rockchip,pins = <0 RK_PB7 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		vcc5v0_power_en: vcc5v0_power_en {
			rockchip,pins = <0 RK_PC0 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
	rtc {
		rtc_int: rtc-int {
			rockchip,pins = <0 RK_PB2 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};
	sata {
		vcc_sata_pwr_en: vcc_sata_pwr_en {
			rockchip,pins = <1 RK_PA2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
	usb {
		vcc5v0_otg_en: vcc5v0_otg_en {
			rockchip,pins = <3 RK_PA6 RK_FUNC_GPIO &pcfg_pull_none>;
		};

	};

	usb-typec {
		usbc0_int: usbc0-int {
			rockchip,pins = <1 RK_PA0 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};

	wireless-bluetooth {
		uart3_gpios: uart3-gpios {
			rockchip,pins = <1 RK_PC2 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		bt_reset_gpio: bt-reset-gpio {
			rockchip,pins = <1 RK_PA7 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		bt_wake_gpio: bt-wake-gpio {
			rockchip,pins = <0 RK_PA0 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		bt_irq_gpio: bt-irq-gpio {
			rockchip,pins = <1 RK_PA6 RK_FUNC_GPIO &pcfg_pull_down>;
		};
	};
	wireless-wlan {
		wifi_host_wake_irq: wifi-host-wake-irq {
			rockchip,pins = <0 RK_PB0 RK_FUNC_GPIO &pcfg_pull_down>;
		};
	};
	sdio-pwrseq {
		wifi_enable_h: wifi-enable-h {
			rockchip,pins = <0 RK_PB2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
};

