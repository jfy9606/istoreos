/{
	rockchip_system_monitor: rockchip-system-monitor {
		compatible = "rockchip,system-monitor";

		rockchip,thermal-zone = "package-thermal";
		status = "disabled";
	};
};

&otp {
	specification_serial_number: specification-serial-number@6 {
		reg = <0x06 0x1>;
		bits = <0 5>;
	};
	customer_demand: customer-demand@22 {
		reg = <0x22 0x1>;
		bits = <4 4>;
	};
};

#include "rk3588s-vpu.dtsi"
#include "rk3588s-gpu.dtsi"
#include "rk3588s-npu.dtsi"
#include "rk3588s-crypto.dtsi"
